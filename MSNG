//https://www.codechef.com/OCT19B/problems/MSNG
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        vector<int>v ;
        int a[n];
        string b[n];
        int x;
        for(int i=0;i<n;i++)
        {
            cin>>a[i]>>b[i];
            if(a[i]!=-1)
                x=i;
        }
        int len=b[x].length();
        //cout<<len;
        int ab[len];
        string ss=b[x];
        int ww=a[x];
        for(int i=0;i<len;i++)
        {
            int ee=(int)ss[i];
            if(ee>=48 && ee<=57)
                ee-=48;
            else
                ee-=55;
            ab[i]=ee;
        }
        //cout<<s;
        int sd=0,zz=0;
        for(int i=len-1;i>=0;i--)
        {
            int q=ab[i];
            //cout<<q<<" ";
            sd+=(q*pow(ww,zz));
            zz++;
        }
        //cout<<sd<<endl;
        int ans=0;
        for(int i=0;i<n;i++)
        {
            int len=b[i].length();
            int aa[len],start=0 ;
            string s=b[i];
            //cout<<start<<endl;
            if(i==x)
                continue;
            for(int k=0;k<len;k++)
            {
                int asciivalue=(int)s[k];
                if(asciivalue>=48 && asciivalue<=57)
                    asciivalue-=48;
                else
                    asciivalue-=55;
                if(start<asciivalue)
                    start=asciivalue;
                aa[i]=asciivalue;
            }
            //cout<<start<<endl;
            int sum=0,power=0,f=0;
            for(int j=start+1;j<=36;j++)
            {
                power=0;
                sum=0;
                //cout<<j<<endl;
                for(int l=len-1;l>=0;l--)
                {
                    if(aa[l]==0)
                        continue;
                    sum+=(aa[l]*(pow(j,power++)));
                }
                cout<<sum<<endl;
                if(sum==sd)
                {
                    f=1;
                    cout<<j<<endl;
                    break;
                } 
            }
            if(f==0)
            {
                ans=1;
                break;
            }
        }
        if(ans==1 && sd>)
1            cout<<-1<<endl;
        else
            cout<<sd<<endl;
    }
}
